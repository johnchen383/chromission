{
  "name": "TabFlo",
  "version": "0.1.3",
  "description": "Helping you to chug through work and flow through tabs!",
  "manifest_version": 2,
  "icons": {
    "128": "/assets/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["/background.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["tabs", "storage"]
}
